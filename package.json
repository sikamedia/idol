    "file-loader": "^0.9.0",
    "history": "^4.3.0",
    "image-webpack-loader": "^3.0.0",
    "immutable": "^3.8.1",
    "imports-loader": "^0.6.5",
    "jquery": "^3.1.1",
    "jshashes": "^1.0.5",
    "json-loader": "^0.5.4",
    "node-uuid": "^1.4.7",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^1.0.0",
    "redux-logger": "^2.7.0",
